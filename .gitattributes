* text=auto eol=lf

override/usr/local/bin/*      text eol=lf executable diff=bash
override/etc/periodic/15min/* text eol=lf executable diff=bash
override/etc/periodic/daily/* text eol=lf executable diff=bash

run text eol=lf executable diff=bash
up  text eol=lf executable diff=bash

.gitattributes text eol=lf -executable export-ignore
.gitignore     text eol=lf -executable export-ignore
